<h1>New Product</h1>

<%= form_for :sell, url: sells_path do |f| %>
  <p>
    <%= f.label :product %><br>
    <%= f.text_field :product %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', sells_path %>